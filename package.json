    "@types/chai": "^4.2.7",
    "@types/clone": "^0.1.30",
    "@types/core-js": "^0.9.43",
    "@types/jest": "^24.9.0",
    "@types/node": "^8.10.59",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
