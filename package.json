{
  "name": "corewar",
  "version": "0.1.2",
  "description": "HTML5 & javascript implementation of the classic game [corewar](https://en.wikipedia.org/wiki/Core_War)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "nyc mocha",
    "build": "tsc",
    "prepublishOnly": "npm run build && npm test"
  },
  "files": [
    "**/dist/index.js",
    "**/dist/"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*[Tt]ests.ts",
      "**/Test*.ts",
      "**/*Helper.ts",
      "dist"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corewar/corewar.git"
  },
  "author": "Gareth Williams",
  "contributors": [
    {
      "name": "Doug McDonald"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corewar/corewar/issues"
  },
  "homepage": "https://github.com/corewar/corewar#readme",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/clone": "^0.1.30",
    "@types/core-js": "^0.9.43",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.10.59",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "npm-cli-login": "^0.1.1",
    "nyc": "^15.0.0",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "ts-mocha": "^1.0.3",
    "ts-node": "^3.3.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "clone": "^2.1.2",
    "core-js": "^2.6.11",
    "node": "^13.6.0",
    "pubsub-js": "^1.8.0"
  }
}
